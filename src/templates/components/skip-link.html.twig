{% macro govukSkipLink(params) %}
<a href="{{ params.href | default('#content') }}" class="govuk-skip-link{%- if params.classes | default(false) %} {{ params.classes }}{% endif -%}"{%- for attribute, value in params.attributes ?? []%} {{ attribute }}="{{ value }}"{% endfor %} data-module="govuk-skip-link">
  {{- params.html | default(false) ? params.html | raw : params.text -}}
</a>
{% endmacro %}
