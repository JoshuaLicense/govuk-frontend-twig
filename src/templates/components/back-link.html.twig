{% macro govukBackLink(params) %}
<a href="{%- if params.href is defined and params.href is not null %}{{ params.href }}{% else %}#{% endif -%}" class="govuk-back-link{%- if params.classes is defined and params.classes is not null %} {{ params.classes }}{% endif -%}"
{%- for attribute, value in params.attributes ?? [] %} {{attribute}}="{{value}}"{% endfor %}>{{ (params.html is defined and params.html is not null ? params.html | raw : (params.text is defined and params.text is not null ? params.text : 'Back')) -}}
</a>
{% endmacro %}
